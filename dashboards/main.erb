<% content_for :title do %>My super sweet dashboard<% end %>
<div class="gridster">
    <ul>
        <li data-row="1" data-col="1" data-sizex="2" data-sizey="1">
            <div data-id="welcome" data-view="Text" data-title="Hello" data-text="This is your shiny new dashboard." data-moreinfo="Protip: You can drag the widgets around!"></div>
        </li>

        <li data-row="1" data-col="1" data-sizex="1" data-sizey="1">
        <div data-id="weather" data-view="Weather"></div>
        </li>

        <li data-row="1" data-col="1" data-sizex="1" data-sizey="1">
        <div data-id="clock" data-view="Clock">></div>
        </li>

        <li data-row="1" data-col="1" data-sizex="1" data-sizey="1">
            <div data-view="Image" data-image="/northeastern2.svg"></div>
        </li>

        <li data-row="1" data-col="1" data-sizex="1" data-sizey="1">
            <div data-view="Countdown" data-title="HOUSING!" data-end="9-Mar-2021 10:40:00"></div>
        </li>
        <li data-row="1" data-col="1" data-sizex="1" data-sizey="2">
            <div data-id="news" data-view="News"></div>
        </li>
    </ul>
</div>

<script type='text/javascript'> $(function() { Dashing.gridsterLayout('[{"col":2,"row":2,"size_x":2,"size_y":1},{"col":2,"row":1,"size_x":1,"size_y":1},{"col":3,"row":1,"size_x":1,"size_y":1},{"col":1,"row":2,"size_x":1,"size_y":1},{"col":1,"row":1,"size_x":1,"size_y":1},{"col":4,"row":1,"size_x":1,"size_y":2}]') }); </script>